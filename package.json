  },
  "license": "MIT",
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "draft-js",
    "draft-js-plugins",
    "plugins",
    "plugin",
    "react",
    "widget",
    "react-component",
    "richtext"
  ],
  "peerDependencies": {
    "draft-js": ">=0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrey-semin/draft-js-list-depth-plugin.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
