  "version": "1.0.1",
  "description": "List depth Plugin for DraftJS",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --ignore __tests__ --out-dir lib",
    "compile": "npm run build",
    "precompile": "npm run clean",
